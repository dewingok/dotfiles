[include]
  path = ~/.git/config_local
[user]
  name = Nick Dewing
[core]
  editor = vim
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = /Users/ndewing/.gitignore_global
[help]
  autocorrect = 0
[merge]
  tool = vimdiff
[push]
  default = simple
[status]
  relativePaths = false
[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
#   header = white
  added = yellow
  updated = yellow
  changed = green
  untracked = cyan
  branch = magenta
  nobranch = red
[color "diff"]
  whitespace = red reverse
[alias]
  st = status --short --branch -unormal
  stp = status --porcelain -unormal
  stn = status
  ci = commit -a
  br = branch
  co = checkout
  vdf = !gitvimdiff
  df = diff --word-diff
  logdiff = log -p --summary
  difflog = log -p --summary
  lg = log --pretty=format:'%C(yellow)%h%C(reset) %C(blue)%an%C(reset) %C(cyan)%cr%C(reset) %s %C(green)%d%C(reset)' --graph --date-order
  ls = ls-files -t -d -o -m -c --directory
  ls-st = ls-files -t -d -o -m -c -s --directory
  dead = log --diff-filter=D --summary
  up = pull
  current-branch = rev-parse --abbrev-ref HEAD
[credential]
	helper = osxkeychain
[pull]
	rebase = false
